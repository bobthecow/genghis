<div class="well">
    <div class="document-actions">
        {{# isGridFile }}
            <a class="btn btn-small btn-primary grid-download" href="{{ downloadUrl }}">Download</a>
        {{/ isGridFile }}
        {{# isGridChunk }}
            <a class="btn btn-small grid-file" href="{{ fileUrl }}">Grid File</a>
        {{/ isGridChunk }}
        <button class="btn btn-small btn-primary save">Save</button>
        <button class="btn btn-small cancel">Cancel</button>
        <button class="btn btn-small edit">Edit</button>
        <button class="btn btn-small btn-danger destroy">Delete</button>
    </div>

    <h3>
        <a class="id" href="{{ url }}">{{ prettyId }}</a>
        {{# prettyTime }}
            <small><time datetime="{{ prettyTime }}">{{ prettyTime }}</time></small>
        {{/ prettyTime }}
    </h3>

    <div class="document"></div>
</div>
